# =========================
# Telegram / Bot Credentials
# =========================

# Your Telegram API ID (from https://my.telegram.org)
API_ID=your_api_id_here

# Your Telegram API Hash (from https://my.telegram.org)
API_HASH=your_api_hash_here

# Bot Token from BotFather
BOT_TOKEN=your_bot_token_here

# =========================
# Owner / Admin IDs
# =========================

# Owner Telegram numeric ID (main user who controls bot)
OWNER_ID=your_telegram_id_here

# Admin IDs (if any, multiple IDs comma-separated)
ADMIN_IDS=123456789,987654321

# =========================
# Database
# =========================

# Database URL (Postgres, MySQL, etc.)
DATABASE_URL=your_database_url_here
